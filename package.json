{
  "name": "git-worktree-handler",
  "displayName": "git-worktree-handler",
  "description": "Enhanced git worktree management with intuitive UI and powerful commands",
  "version": "1.0.5",
  "publisher": "tryToDEv",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "SCM Providers"
  ],
  "keywords": [
    "git",
    "worktree",
    "git worktree",
    "branch",
    "source control"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitWorktree.list",
        "title": "List Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.add",
        "title": "Add New Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.addExistingBranch",
        "title": "Add Worktree from Existing Branch",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.remove",
        "title": "Remove Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.switch",
        "title": "Switch to Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.openInNewWindow",
        "title": "Open Worktree in New Window",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.prune",
        "title": "Prune Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.refresh",
        "title": "Refresh Worktree List",
        "category": "Git Worktree",
        "icon": "$(refresh)"
      },
      {
        "command": "gitWorktree.openTerminal",
        "title": "Open Terminal in Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.switchBranch",
        "title": "Switch Branch in Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.createBranch",
        "title": "Create Branch in Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.pull",
        "title": "Pull Changes in Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.push",
        "title": "Push Changes from Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.copyPath",
        "title": "Copy Worktree Path",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.switchMainBranch",
        "title": "Switch Main Worktree branch",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.addNote",
        "title": "Add/Edit Note",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.compare",
        "title": "Compare with Current Worktree",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.multiSearch",
        "title": "Search Across All Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.cleanupMerged",
        "title": "Cleanup Merged Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.quickActions",
        "title": "Quick Actions",
        "category": "Git Worktree",
        "icon": "$(rocket)"
      },
      {
        "command": "gitWorktree.pullAll",
        "title": "Pull All Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.pushAllDirty",
        "title": "Push All Dirty Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.installDepsAll",
        "title": "Install Dependencies in All Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.syncSettings",
        "title": "Sync Settings to All Worktrees",
        "category": "Git Worktree"
      },
      {
        "command": "gitWorktree.refreshHealth",
        "title": "Refresh Health Monitor",
        "category": "Git Worktree",
        "icon": "$(refresh)"
      },
      {
        "command": "gitWorktree.refreshTimeline",
        "title": "Refresh Timeline",
        "category": "Git Worktree",
        "icon": "$(refresh)"
      },
      {
        "command": "gitWorktree.refreshGraph",
        "title": "Refresh Dependency Graph",
        "category": "Git Worktree",
        "icon": "$(refresh)"
      },
      {
        "command": "gitWorktree.moveFiles",
        "title": "Move Files to Another Worktree",
        "category": "Git Worktree",
        "icon": "$(arrow-right)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "git-worktree-explorer",
          "title": "Git Worktrees",
          "icon": "resources/worktree-icon.png"
        }
      ]
    },
    "views": {
      "git-worktree-explorer": [
        {
          "id": "gitWorktreeView",
          "name": "Worktrees",
          "icon": "resources/worktree-icon.png",
          "contextualTitle": "Git Worktrees"
        },
        {
          "id": "gitHealthView",
          "name": "Health Monitor",
          "icon": "$(pulse)",
          "contextualTitle": "Worktree Health"
        },
        {
          "id": "gitTimelineView",
          "name": "Timeline",
          "icon": "$(history)",
          "contextualTitle": "Worktree Timeline"
        },
        {
          "id": "gitGraphView",
          "name": "Dependencies",
          "icon": "$(graph)",
          "contextualTitle": "Dependency Graph"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "gitWorktree.quickActions",
          "when": "view == gitWorktreeView",
          "group": "navigation@0"
        },
        {
          "command": "gitWorktree.refresh",
          "when": "view == gitWorktreeView",
          "group": "navigation@1"
        },
        {
          "command": "gitWorktree.add",
          "when": "view == gitWorktreeView"
        },
        {
          "command": "gitWorktree.refreshHealth",
          "when": "view == gitHealthView",
          "group": "navigation"
        },
        {
          "command": "gitWorktree.refreshTimeline",
          "when": "view == gitTimelineView",
          "group": "navigation"
        },
        {
          "command": "gitWorktree.refreshGraph",
          "when": "view == gitGraphView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "gitWorktree.switch",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "inline@1"
        },
        {
          "command": "gitWorktree.openInNewWindow",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "1_open@1"
        },
        {
          "command": "gitWorktree.openTerminal",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "1_open@2"
        },
        {
          "command": "gitWorktree.copyPath",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "1_open@3"
        },
        {
          "command": "gitWorktree.switchBranch",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "2_branch@1"
        },
        {
          "command": "gitWorktree.createBranch",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "2_branch@2"
        },
        {
          "command": "gitWorktree.pull",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "3_sync@1"
        },
        {
          "command": "gitWorktree.push",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "3_sync@2"
        },
        {
          "command": "gitWorktree.remove",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "4_manage@1"
        },
        {
          "command": "gitWorktree.addNote",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "4_manage@2"
        },
        {
          "command": "gitWorktree.compare",
          "when": "view == gitWorktreeView && viewItem == worktree",
          "group": "1_open@4"
        }
      ],
      "commandPalette": [
        {
          "command": "gitWorktree.refresh",
          "when": "gitOpenRepositoryCount != 0"
        }
      ]
    },
    "configuration": {
      "title": "tryToDEv",
      "properties": {
        "gitWorktree.defaultLocation": {
          "type": "string",
          "default": "",
          "description": "Default location for new worktrees (leave empty to use parent of main repository)"
        },
        "gitWorktree.autoRefresh": {
          "type": "boolean",
          "default": true,
          "description": "Automatically refresh worktree list when changes are detected"
        },
        "gitWorktree.showBranchNames": {
          "type": "boolean",
          "default": true,
          "description": "Show branch names in the worktree list"
        },
        "gitWorktree.sortOrder": {
          "type": "string",
          "enum": [
            "default",
            "branch",
            "path"
          ],
          "default": "default",
          "description": "Sort order for worktrees in the view"
        },
        "gitWorktree.confirmBeforeRemove": {
          "type": "boolean",
          "default": true,
          "description": "Ask for confirmation before removing a worktree"
        },
        "gitWorktree.showStatusBar": {
          "type": "boolean",
          "default": true,
          "description": "Show the current worktree in the status bar"
        },
        "gitWorktree.hooks": {
          "type": "object",
          "default": {},
          "description": "Lifecycle hooks to run on worktree events",
          "properties": {
            "onCreate": {
              "type": "string",
              "description": "Command to run after creating a worktree. Use {path}, {branch}, {worktree} as variables."
            },
            "onDelete": {
              "type": "string",
              "description": "Command to run before deleting a worktree"
            },
            "onSwitch": {
              "type": "string",
              "description": "Command to run after switching to a worktree"
            }
          }
        },
        "gitWorktree.health.checkInterval": {
          "type": "number",
          "default": 900000,
          "description": "Health check interval in milliseconds (default: 15 minutes)"
        },
        "gitWorktree.sync.enabled": {
          "type": "boolean",
          "default": false,
          "description": "Enable settings sync across worktrees"
        },
        "gitWorktree.sync.autoSync": {
          "type": "boolean",
          "default": false,
          "description": "Automatically sync settings when they change"
        },
        "gitWorktree.fileTransfer.confirmOverwrite": {
          "type": "boolean",
          "default": true,
          "description": "Ask for confirmation before overwriting existing files in target worktree"
        },
        "gitWorktree.fileTransfer.autoStage": {
          "type": "boolean",
          "default": true,
          "description": "Automatically stage files in target worktree if they were staged in source"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "typescript": "^5.1.6",
    "@vscode/vsce": "^2.19.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/Shivam-20/gitWorkTreeManger"
  },
  "bugs": {
    "url": "https://github.com/Shivam-20/gitWorkTreeManger/issues"
  },
  "icon": "resources/icon.png",
  "license": "MIT"
}